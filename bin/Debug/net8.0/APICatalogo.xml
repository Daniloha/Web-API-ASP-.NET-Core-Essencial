            <returns> Uma categoria</returns>
        </member>
        <member name="M:APICatalogo.Controllers.CategoriasController.Post(APICatalogo.DTOs.CategoriaDTO)">
             <summary>
             Cria uma nova categoria
             </summary>
             <remarks>
             Exemplo de request:
            
                 POST /Categorias
                 {
                     "Nome": "Nome da categoria"
                     "ImagemUrl": "http://www.imagem_da_categoria.jpg"
                 }
             </remarks>
             <param name="categoriaDto">objeto Categoria</param>
             <returns>O objeto categoria criado</returns>
        </member>
